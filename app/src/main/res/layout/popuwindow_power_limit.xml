<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/xa450"
    android:layout_height="@dimen/xa230"
    android:background="@drawable/shape_white_corner_bg"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="限制功率1kw充电"
        android:textColor="@color/title_1"
        android:textSize="@dimen/xa26" />

    <TextView
        android:id="@+id/tv_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/xa5"
        android:text="@string/m限制时间段"
        android:textColor="@color/title_1"
        android:textSize="@dimen/xa24" />

    <TextView
        android:id="@+id/tv_confirm"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xa60"
        android:gravity="center"
        android:layout_marginTop="@dimen/xa80"
        android:text="@string/m9确定"
        android:textColor="@color/green_1"
        android:textSize="@dimen/xa35" />


</LinearLayout>
