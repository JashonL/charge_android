<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ll_preinstall"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/xa60"
    android:orientation="vertical"
    android:visibility="gone"
    app:layout_constraintTop_toBottomOf="@+id/ll_function">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/xa37"
        android:text="@string/m203预约充电"
        android:textColor="#FF343434" />

    <RelativeLayout
        android:layout_width="@dimen/xa690"
        android:layout_height="@dimen/xa200"
        android:layout_marginStart="@dimen/xa30"
        android:layout_marginTop="@dimen/xa30"
        android:layout_marginEnd="@dimen/xa30"
        android:background="@drawable/shape_preinstall_bg">

        <!--        <Switch-->
        <!--            android:id="@+id/sw_preinstall"-->
        <!--            android:layout_width="wrap_content"-->
        <!--            android:layout_height="wrap_content"-->
        <!--            android:layout_alignParentEnd="true"-->
        <!--            android:layout_marginTop="@dimen/xa30"-->
        <!--            android:layout_marginEnd="@dimen/xa40"-->
        <!--            android:checked="true"-->
        <!--            android:thumb="@drawable/shape_switch_thumb"-->
        <!--            android:track="@drawable/selector_switch_track" />-->

        <!--        <ImageView-->
        <!--            android:id="@+id/iv_preinstall_delete"-->
        <!--            android:layout_width="@dimen/xa32"-->
        <!--            android:layout_height="@dimen/xa32"-->
        <!--            android:layout_alignParentEnd="true"-->
        <!--            android:layout_marginTop="@dimen/xa30"-->
        <!--            android:layout_marginEnd="@dimen/xa40"-->
        <!--            android:src="@drawable/ic_cancel_preinstall" />-->

        <ImageView
            android:id="@+id/iv_preinstall_type"
            android:layout_width="@dimen/xa88"
            android:layout_height="@dimen/xa88"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/xa40"
            android:src="@drawable/ic_time_preinstall" />

        <LinearLayout
            android:id="@+id/ll_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/xa40"
            android:layout_marginTop="@dimen/xa56"
            android:layout_marginEnd="@dimen/xa37"
            android:layout_toEndOf="@+id/iv_preinstall_type"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_start_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/m204开始时间"
                android:textColor="#000000"
                android:textSize="@dimen/xa30"
                android:textStyle="bold" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_time"
            android:layout_marginStart="@dimen/xa40"
            android:layout_marginEnd="@dimen/xa37"
            android:layout_toEndOf="@+id/iv_preinstall_type"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="00:00-00:00"
                android:textColor="#FF009DFF"
                android:textSize="@dimen/xa36" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/xa12"
                android:layout_weight="1"
                android:text=""
                android:textColor="#FF888888"
                android:textSize="@dimen/xa30" />

            <CheckBox
                android:id="@+id/cb_everyday"
                android:layout_width="@dimen/xa32"
                android:layout_height="@dimen/xa32"
                android:button="@drawable/selector_cb_state"
                android:checked="false"
                android:enabled="false" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/m186每天"
                android:textColor="#FF666666"
                android:textSize="@dimen/xa30" />

        </LinearLayout>


    </RelativeLayout>

    <ImageView
        android:id="@+id/iv_exception_type"
        android:layout_width="@dimen/xa302"
        android:layout_height="@dimen/xa114"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/xa30"
        android:src="@drawable/ic_yuyue" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/xa58"
        android:layout_marginBottom="@dimen/xa60"
        android:paddingStart="@dimen/xa30"
        android:paddingEnd="@dimen/xa30"
        android:text="@string/m167现在正处于时段预约状态" />


</LinearLayout>